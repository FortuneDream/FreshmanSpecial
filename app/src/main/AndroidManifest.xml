<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.excitingboat.freshmanspecial">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />


    <application
        android:name=".App"
        android:allowBackup="true"
        android:icon="@drawable/github"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".view.activity.FreshmanMainActivity"
            android:label="@string/freshman_title">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".view.activity.FreshmanLoginActivity"
            android:label="@string/title_activity_freshman_login"
            />
        <activity android:name=".view.activity.FreshmenGuideActivity"
            android:label="@string/freshman_guide"
            />
        <activity android:name=".view.activity.FreshmenBigDataActivity"
            android:label="@string/freshman_big_data"
            />
        <activity android:name=".view.activity.FreshmenCQUPTStyleActivity"
            android:label="@string/freshman_about_cqupt"
            />
    </application>

</manifest>